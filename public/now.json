{
  "version": 2,
  "name": "spaced-repetition",
  "alias": "w-a-a-spaced-repetition",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {

        "src": "^/favicon.ico$",

        "dest": "/favicon.ico"

      },
      {
         "src": "^/manifest.json", 
         "dest": "/manifest.json" 
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}